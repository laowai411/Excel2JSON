	"iconOpens" :
	{
		//主界面上的图标,首先在MainConst类里面注册一个名称，该名称唯一
		//可以配置strengthOpen = true属性，如果配置
		//每次点击都是开启面板，要配置alwaysOpen = true，
		//否则将是取反处理成打开->关闭->打开
		//一种是静态的图标,表1表示，指定位置不受其他影响
		//其他的将按照组来排位置
		//一种是主界面下方有新增信息时要移动并排列的图标，比如邀请组队,交易等，用2表示。
		//类型为2将会按照类型为2的进行排序。
		//如果将起点和终点设置成为相同的则将不进行移动
		//offSet表示非1类型下，多个图标布局时，与图标位置成一定差值一个常量,
		//正数表示从左到右排，负数表示从右到左排
		//useMoveEffect是否使用移动效果，不使用则直接布局
		"iconTypes" :
		{
			"1" :
			{
				//静态图标
				//图标位置在每一个静态图标内配置startPoint属性和Target属性相同
				//startPoint位置：X为正则表示相对正的坐标，X为负则表示相对舞台右侧向左的坐标，Y同理。
				"totalLimit":0,
				"position":["left",0,"top",0] //容器生成的位置
			},
			"2" :
			{
				//主界面下方的图标
				"sequence" : [] , //将配置一个顺序按照这个顺序来显示图标
				"maxCount" : 9, //一行最多排个图标
				"startPoint" : ["left",100,"top",200] , //开始出发点,此位置相对于舞台
				"targetPoint" : ["right",-888,"bottom",-100] , //目标点,此位置相对于容器
				"offSet" : [39, 39] ,
				"position":["right",-1,"bottom",-1], //容器生成的位置
				"tailEffect":true,
				"totalLimit":0,  //总共显示的数量
				"useMoveEffect" : true //是否使用移动特效来处理这些图标,这个属性和sequence冲突
			},
			"3" :
			{
				//小地图左边的图标
				"sequence" : ["NewFeatureIcon","PrivilegeIcon","betaActivityIcon","OpenServerName","equipTargetIcon","supportIcon","vipHelperBookIcon","vipHelperRoleIcon","canGetRewardIcon","onLineRewardIcon","getRewardTomorrowIcon","VipDueIcon","crossIcon","huaShenCopyIcon","bibleCopyIcon","kingIcon","chestIcon","chest2Icon","FirstRechargeIcon","buyTask","misteryShopIcon","bottleIcon","friendRecommendIcon","treasureIcon","vipRewardIcon","crossServerGodsIcon","godsRankIcon","campFightIcon","guildPartyIcon","cityWarRewardIcon","WorldBossIconName","WorldJiuYouBossIconName","ProtTsDoubTimeIconName","questionIcon","hotSpringIcon","SeventFariyIconName","godsAreaRewardIcon","godsAreaIcon","guildGod","pveRewardIcon","campRewardIcon","questionRewardIcon","preindulgenceIcon","taskAddFavorIcon","guildFightTimeIcon","guildFightRewardIcon","PVPTaskRewardIcon","PVPTaskEnterIcon","guildGiftIconS","SeventFariyGetRewIconName","godsRingIcon","godsRingRewardIcon","BackIcon","ConsumeIcon","RechargeIcon","fundSIcon","fundIcon","LuckWheelIcon","SupportExpIcon","specialActivityIcon","specialSellIcon","mergeActivityIcon","GoldVipActivityIcon","mergeSellIcon","StarTreasureHouseIcon","MonthTreasureHouseIcon","crossServerGodsRewardIcon","crossServerCampRewardIcon","flowerRankIcon","cityWarIcon","cityWarWelfareIcon","holidayBagsIcon","BufferinActivityIcon","WeekendSaleIcon","ConsumeRankIcon","MingRenTangRankIcon","KillMonsterIcon","oneDiscountShopIcon","ConsumeBagIcon","MonsterCardIcon","PlatformIcon","SWHSRankIcon","MountSpiritRankIcon","ChouJiangIcon","GuildConsumeIcon","ActivityConsumeIcon","ActivityRechargeIcon","ActivityMarryIcon","OpenServerRebateName","SBRankIcon","OldPlayerReturnIcon","VipMemberYYIcon","VIPGift360Icon"] , //将配置一个顺序按照这个顺序来显示图标
				"maxCount" : 9, //一行最多排个图标
				"startPoint" : ["center",-31,"top",280] , //开始出发点,此位置相对于容器坐标
				"targetPoint" : ["right",-260,"top",25] , //目标点
				"position":["right",-1,"top",0],
				"totalLimit":18,
				"offSet" : [55, 68] , //图标的宽度和高度
				"useMoveEffect" : false //是否使用移动特效来处理这些图标
				
			}
		},
